sort one array in increasing order, one in decreasing order
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        Integer a[] = {1,2,2,1};
        int b[] = { 3,3,3,4 };
        int k = 10;
        Arrays.sort(a,Collections.reverseOrder());
        Arrays.sort(b);
        int flag=0;
        for(int i=0;i<b.length;i++)
        {
            if(a[i]+b[i]<k)
            {
                System.out.println("no");
                flag=1;
                break;
            }
        }
        if(flag!=1)
            System.out.println("yes");
 
    }
}
